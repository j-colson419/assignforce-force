<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>StartDateNotInPast</fullName>
    <active>true</active>
    <errorConditionFormula>StartDate__c &lt; TODAY()</errorConditionFormula>
    <errorDisplayField>StartDate__c</errorDisplayField>
    <errorMessage>Cannot create a new Batch in the past. May only create Batches which start today or later.</errorMessage>
</ValidationRule>
